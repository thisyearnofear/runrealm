{
  "name": "@runrealm/shared-blockchain",
  "version": "2.0.0",
  "description": "Blockchain services for RunRealm platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./services/*": {
      "import": "./dist/services/*.js",
      "require": "./dist/services/*.js",
      "types": "./dist/services/*.d.ts"
    },
    "./types/*": {
      "import": "./dist/types/*.js",
      "require": "./dist/types/*.js",
      "types": "./dist/types/*.d.ts"
    }
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "dependencies": {
    "ethers": "^6.13.0",
    "@zetachain/protocol-contracts": "^13.0.0",
    "@zetachain/toolkit": "^16.1.1",
    "@runrealm/shared-core": "^2.0.0"
  }
}