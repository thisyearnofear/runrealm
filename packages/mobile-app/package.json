{
  "name": "@runrealm/mobile-app",
  "version": "2.0.0",
  "description": "Mobile application for RunRealm platform",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "ios": "expo start --ios",
    "android": "expo start --android",
    "web": "expo start --web",
    "build": "tsc",
    "build:ios": "eas build --profile production --platform ios",
    "build:android": "eas build --profile production --platform android",
    "submit:ios": "eas submit --platform ios",
    "test": "jest",
    "clean": "rm -rf node_modules && npm install"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^7.5.0",
    "@react-navigation/native": "^7.1.18",
    "@runrealm/shared-blockchain": "^2.0.0",
    "@runrealm/shared-core": "^2.0.0",
    "@runrealm/shared-types": "^2.0.0",
    "@runrealm/shared-utils": "^2.0.0",
    "expo": "~50.0.0",
    "expo-constants": "~15.4.0",
    "expo-device": "~5.9.0",
    "expo-location": "~16.5.0",
    "expo-status-bar": "~1.11.0",
    "react": "18.2.0",
    "react-native": "0.73.0",
    "react-native-gesture-handler": "^2.29.0",
    "react-native-maps": "1.10.0",
    "react-native-reanimated": "^4.1.3",
    "react-native-safe-area-context": "^5.6.1",
