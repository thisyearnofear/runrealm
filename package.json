{
  "name": "runrealm",
  "version": "2.0.0",
  "description": "Cross-chain fitness GameFi app - claim, trade, and defend real-world running territories as NFTs",
  "main": "bundle.js",
  "scripts": {
    "test": "jasmine-ts --config=jasmine.json",
    "test:unit": "jest --config=jest.config.js",
    "test:performance": "npm run build && node scripts/performance-test.js",
    "watch": "nodemon --ext ts --exec 'npm run coverage'",
    "lint": "tslint --project tslint.json",
    "lint:fix": "tslint --project tslint.json --fix",
    "serve": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack serve --config webpack.config.js",
    "init": "sh ./sh/dev-init.sh",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack --mode=production --config webpack.config.js",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "build:prod": "npm run build",
    "coverage": "nyc -r lcov -e .ts -x \"*.spec.ts\" npm run test",
    "clean": "rimraf public/*.js public/*.map",
    "typecheck": "tsc --noEmit",
    "web3:setup": "node scripts/web3-setup.js",
    "contracts:compile": "npx hardhat compile",
    "contracts:deploy": "npx hardhat run scripts/deploy-contracts.js",
    "contracts:deploy:testnet": "npx hardhat run scripts/deploy-contracts.js --network zetachain_testnet",
    "contracts:deploy:local": "npx hardhat run scripts/deploy-contracts.js --network localhost",
    "contracts:verify": "npx hardhat verify --network zetachain_testnet",
    "test:contracts": "npx hardhat test",
    "test:web3": "npx hardhat test",
    "start": "npm run serve",
    "dev": "npm run serve",
    "deploy:vercel": "vercel --prod",
    "deploy:staging": "vercel",
    "lighthouse": "lighthouse http://localhost:9000 --output html --output json --output-path ./reports/lighthouse",
    "contracts:status": "npx hardhat run scripts/check-deployment-status.js --network zetachain_testnet",
    "contracts:check": "npx hardhat run scripts/check-deployment-status.js --network zetachain_testnet",
    "contracts:interact": "npx hardhat run scripts/interact-with-contracts.js --network zetachain_testnet",
    "contracts:info": "cat deployments/zetachain_testnet-deployment.json | jq"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeffbdye/RunRealm.git"
  },
  "author": "Jeff Dye",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/jeffbdye/RunRealm/issues"
  },
  "homepage": "https://github.com/jeffbdye/RunRealm#readme",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@types/jasmine": "3.3.14",
    "@types/mapbox__mapbox-sdk": "0.13.2",
    "@types/mapbox-gl": "2.4.2",
    "@types/node": "^20.0.0",
    "@types/react": "^19.10.10",
    "cross-env": "^10.0.0",
    "hardhat": "^2.17.0",
    "html-webpack-plugin": "^5.6.4",
    "jasmine": "3.9.0",
    "jasmine-console-reporter": "2.0.1",
    "jasmine-ts": "0.4.0",
    "lighthouse": "^12.0.0",
    "nodemon": "2.0.12",
    "nyc": "15.1.0",
    "ts-node": "10.2.1",
    "tslint": "5.18.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-dev-server": "4.9.2"
  },
  "dependencies": {
    "@google/generative-ai": "^0.17.0",
    "@mapbox/mapbox-sdk": "0.13.2",
    "@openzeppelin/contracts": "^5.4.0",
    "@turf/turf": "^6.5.0",
    "@zetachain/protocol-contracts": "^13.0.0",
    "@zetachain/toolkit": "^16.1.0",
    "dotenv": "^17.2.1",
    "ethers": "^6.7.0",
    "mapbox-gl": "2.4.1",
    "ngeohash": "^0.6.3",
    "ts-loader": "^9.4.0",
    "typescript": "^5.0.4",
    "uuid": "^9.0.0",
    "webpack": "5.53.0",
    "webpack-cli": "4.8.0",
    "webpack-merge": "5.8.0"
  }
}
