{
  "name": "runrealm",
  "version": "2.0.0",
  "description": "ZetaChain fitness GameFi app - claim and defend real-world running territories as NFTs",
  "main": "bundle.js",
  "scripts": {
    "test": "jasmine-ts --config=jasmine.json",
    "test:unit": "jest --config=jest.config.js",
    "test:performance": "npm run build && node scripts/performance-test.js",
    "watch": "nodemon --ext ts --exec 'npm run coverage'",
    "lint": "echo 'Linting disabled - TSLint removed (deprecated)'",
    "lint:fix": "echo 'Linting disabled - TSLint removed (deprecated)'",
    "serve": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack serve --config webpack.config.js",
    "init": "echo 'Manual setup required - see docs/DEVELOPER_GUIDE.md'",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack --mode=production --config webpack.config.js",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "build:prod": "npm run build",
    "coverage": "nyc -r lcov -e .ts -x \"*.spec.ts\" npm run test",
    "clean": "node scripts/cleanup.js",
    "clean:legacy": "rimraf public/*.js public/*.map",
    "typecheck": "tsc --noEmit",
    "web3:setup": "node scripts/web3-setup.js",
    "contracts:compile": "npx hardhat compile",
    "contracts:deploy": "npx hardhat run scripts/deploy-universal.js --network zetachain_testnet",
    "contracts:deploy:testnet": "npx hardhat run scripts/deploy-universal.js --network zetachain_testnet",
    "contracts:deploy:mainnet": "npx hardhat run scripts/deploy-universal.js --network zetachain_mainnet",
    "contracts:verify": "npx hardhat verify --network zetachain_testnet",
    "test:contracts": "npx hardhat test",
    "test:web3": "npx hardhat test",
    "start": "npm run serve",
    "dev": "npm run serve",
    "server": "node server.js",
    "lighthouse": "lighthouse http://localhost:9000 --output html --output json --output-path ./reports/lighthouse",
    "contracts:status": "echo 'Current deployment info:' && cat deployments/zetachain_testnet-universal.json 2>/dev/null || echo 'No deployment found. Run: npm run contracts:deploy:testnet'",
    "contracts:info": "cat deployments/zetachain_testnet-universal.json 2>/dev/null || echo 'No deployment found'",
    "setup:mapbox": "node scripts/setup-mapbox.js",
    "setup:keys": "node scripts/setup-secure-keys.js",
    "favicon": "node scripts/generate-all-favicons.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeffbdye/RunRealm.git"
  },
  "author": "Jeff Dye",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/jeffbdye/RunRealm/issues"
  },
  "homepage": "https://github.com/jeffbdye/RunRealm#readme",
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.1.0",
    "@nomicfoundation/hardhat-ethers": "^3.1.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.1.0",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@nomicfoundation/hardhat-verify": "^1.1.1",
    "@typechain/ethers-v6": "^0.4.3",
    "@typechain/hardhat": "^8.0.3",
    "@types/chai": "^4.3.20",
    "@types/geojson": "^7946.0.16",
    "@types/jasmine": "3.3.14",
    "@types/mapbox__mapbox-sdk": "0.13.2",
    "@types/mapbox-gl": "2.4.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.0.0",
    "@types/react": "^19.1.10",
    "@types/uuid": "^10.0.0",
    "chai": "^4.5.0",
    "favicons": "^7.2.0",
    "hardhat": "^2.17.0",
    "hardhat-gas-reporter": "^1.0.10",
    "jasmine": "3.9.0",
    "jasmine-console-reporter": "2.0.1",
    "jasmine-ts": "0.4.0",
    "lighthouse": "^12.0.0",
    "nodemon": "2.0.12",
    "nyc": "15.1.0",
    "solidity-coverage": "^0.8.16",
    "to-ico": "^1.1.5",
    "ts-node": "10.2.1",
    "tslint": "5.18.0",
    "typechain": "^8.3.2",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-dev-server": "4.9.2"
  },
  "dependencies": {
    "@google/generative-ai": "^0.17.0",
    "@mapbox/mapbox-sdk": "0.13.2",
    "@openzeppelin/contracts": "^5.4.0",
    "@openzeppelin/contracts-upgradeable": "^5.4.0",
    "@turf/turf": "^6.5.0",
    "@zetachain/protocol-contracts": "^13.0.0",
    "buffer": "^6.0.3",
    "copy-webpack-plugin": "^13.0.1",
    "cross-env": "^10.0.0",
    "css-loader": "^7.1.2",
    "dotenv": "^17.2.1",
    "ethers": "^6.13.0",
    "express": "^5.1.0",
    "html-webpack-plugin": "^5.6.4",
    "mapbox-gl": "2.4.1",
    "ngeohash": "^0.6.3",
    "process": "^0.11.10",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.4.0",
    "typescript": "^5.0.4",
    "uuid": "^9.0.0",
    "webpack": "5.53.0",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "5.8.0"
  }
}